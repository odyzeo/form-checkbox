{
    "name": "@odyzeo/form-checkbox",
    "version": "1.5.1",
    "description": "Odyzeo form checkbox component with Vue",
    "keywords": [
        "odyzeo",
        "form",
        "checkbox",
        "vue"
    ],
    "author": "Odyzeo <tomas.zamba@odyzeo.com, peter.baricic@odyzeo.com> (https://odyzeo.com)",
    "publishConfig": {
        "access": "public"
    },
    "license": "ISC",
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "npm run build:es & vue-cli-service build --target lib --name form-checkbox ./src/plugin/FormCheckbox.js",
        "build:es": "rollup --config rollup.config.js --format es --file dist/form-checkbox.esm.js",
        "lint": "vue-cli-service lint",
        "start": "vue-cli-service serve"
    },
    "main": "dist/form-checkbox.common.js",
    "module": "dist/form-checkbox.esm.js",
    "unpkg": "dist/form-checkbox.umd.min.js",
    "style": "dist/form-checkbox.css",
    "repository": {
        "type": "git",
        "url": "https://github.com/odyzeo/form-checkbox.git"
    },
    "files": [
        "dist/form-checkbox.common.js",
        "dist/form-checkbox.umd.min.js",
        "dist/form-checkbox.umd.js",
        "dist/form-checkbox.esm.js",
        "dist/form-checkbox.css",
        "src"
    ],
    "dependencies": {},
    "devDependencies": {
        "@vue/cli-plugin-babel": "~4.2.0",
        "@vue/cli-plugin-eslint": "~4.2.0",
        "@vue/cli-service": "~4.2.0",
        "@vue/eslint-config-airbnb": "^5.0.2",
        "babel-eslint": "^10.0.3",
        "core-js": "^3.6.4",
        "eslint": "^6.7.2",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-vue": "^6.1.2",
        "less": "^3.0.4",
        "less-loader": "^5.0.0",
        "rollup": "^1.31.1",
        "rollup-plugin-buble": "^0.19.8",
        "rollup-plugin-commonjs": "^10.1.0",
        "rollup-plugin-css-only": "^2.0.0",
        "rollup-plugin-vue": "^4.7.2",
        "vue": "^2.6.11",
        "vue-template-compiler": "^2.6.11"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "browser": true,
            "es6": true
        },
        "extends": [
            "plugin:vue/recommended",
            "@vue/airbnb"
        ],
        "rules": {
            "indent": 0,
            "vue/script-indent": [
                2,
                4
            ],
            "vue/html-indent": [
                2,
                4
            ],
            "vue/require-v-for-key": 1,
            "vue/html-self-closing": [
                2,
                {
                    "html": {
                        "normal": "never",
                        "component": "never"
                    }
                }
            ],
            "camelcase": 0,
            "import/extensions": [
                2,
                "ignorePackages",
                {
                    "js": "never"
                }
            ],
            "import/prefer-default-export": 0,
            "no-param-reassign": [
                2,
                {
                    "props": false
                }
            ],
            "no-lonely-if": 0,
            "arrow-parens": [
                2,
                "as-needed",
                {
                    "requireForBlockBody": true
                }
            ]
        },
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions"
    ],
    "jsdelivr": "dist/form-checkbox.umd.min.js"
}
