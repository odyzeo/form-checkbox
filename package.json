{
  "name": "@odyzeo/form-checkbox",
  "version": "1.5.0",
  "description": "Odyzeo form checkbox component with Vue",
  "author": "Odyzeo <tomas.zamba@odyzeo.com, peter.baricic@odyzeo.com> (https://odyzeo.com)",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --fix",
    "build-lib": "vue-cli-service build --modern --target lib --name form-checkbox ./src/components/FormCheckbox.vue",
    "demo": "vue-cli-service demo",
    "docs": "npm run docs:serve",
    "docs:build": "vue-cli-service docs --mode build",
    "docs:serve": "vue-cli-service docs --mode serve",
    "prepublish": "vue-cli-service lint && vue-cli-service docs --mode build && vue-cli-service build",
    "start": "vue-cli-service serve"
  },
  "main": "dist/form-checkbox.common.js",
  "module": "dist/form-checkbox.esm.js",
  "browser": "dist/form-checkbox.umd.js",
  "unpkg": "dist/form-checkbox.umd.min.js",
  "files": [
    "dist/form-checkbox.common.js",
    "dist/form-checkbox.umd.min.js",
    "dist/form-checkbox.umd.js",
    "dist/form-checkbox.esm.js",
    "src"
  ],
  "dependencies": {
    "axios": "^0.19.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.2.2",
    "@vue/cli-service": "^4.2.2",
    "@vue/eslint-config-airbnb": "^5.0.2",
    "less": "^3.11.1",
    "less-loader": "^5.0.0",
    "vue-cli-plugin-p11n": "~0.4.0",
    "vue-template-compiler": "^2.6.11",
    "vue": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true,
      "es6": true
    },
    "extends": [
      "plugin:vue/recommended",
      "@vue/airbnb"
    ],
    "rules": {
      "indent": 0,
      "vue/script-indent": [
        2,
        4
      ],
      "vue/html-indent": [
        2,
        4
      ],
      "vue/require-v-for-key": 1,
      "vue/html-self-closing": [
        2,
        {
          "html": {
            "normal": "never",
            "component": "never"
          }
        }
      ],
      "camelcase": 0,
      "import/extensions": [
        2,
        "ignorePackages",
        {
          "js": "never",
          "vue": "never"
        }
      ],
      "import/prefer-default-export": 0,
      "no-param-reassign": [
        2,
        {
          "props": false
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "jsdelivr": "dist/form-checkbox.umd.min.js",
  "keywords": [
    "odyzeo",
    "form",
    "checkbox",
    "vue"
  ],
  "license": "ISC",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/odyzeo/form-checkbox.git"
  },
  "sideeffects": false,
  "style": "dist/form-checkbox.css"
}
